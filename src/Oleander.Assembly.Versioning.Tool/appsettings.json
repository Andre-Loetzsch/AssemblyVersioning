{
  "loggerTypes": {
    "consoleSink": {
      "Type": "Oleander.Extensions.Logging.Console.ColoredConsoleSink, Oleander.Extensions.Logging.Console",
      "Instance": null
    },
    "fileSink": {
      "Type": "Oleander.Extensions.Logging.File.FileSink, Oleander.Extensions.Logging.File",
      "Instance": null
    }
  },

  "fileSink": {

    "Name": "fileSink",
    "OverrideExistingFile": "true",
    "FileNameTemplate": "{baseDirectory}/Logging/assemblyVersioningTool.log",
    "TextFormatterType": "Oleander.Extensions.Logging.TextFormatters.ShortVerticalTextFormatter, Oleander.Extensions.Logging.TextFormatters",
    "Categories": [
      "*"
    ],
    "LogLevel": "Information"
  },

  "consoleSink": {
    "Name": "consoleSink",
    "Categories": [
      "*",
      "Program",
      "Tentakel.Opc.Client.WorkerService"
    ],
    "LogLevel": "Debug",
    "TextFormatterType": "Oleander.Extensions.Logging.TextFormatters.ShortVerticalTextFormatter, Oleander.Extensions.Logging.TextFormatters"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Olenander": "Debug"
    },
    "Olenander": {
      "LogLevel": {
        "Default": "Debug",
        "Program": "Debug"
      }
    }
  }
}